{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "splitPane",
                "split": "right"
            },
            "keys": "alt+shift+5"
        },
        {
            "command": 
            {
                "action": "prevTab"
            },
            "keys": "ctrl+pgup"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "nextInOrder"
            },
            "keys": "alt+shift+o"
        },
        {
            "command": "unbound",
            "keys": "alt+enter"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "down"
            },
            "keys": "alt+shift+'"
        },
        {
            "command": 
            {
                "action": "nextTab"
            },
            "keys": "ctrl+pgdn"
        },
        {
            "command": 
            {
                "action": "closeTab"
            },
            "keys": "ctrl+f4"
        }
    ],
